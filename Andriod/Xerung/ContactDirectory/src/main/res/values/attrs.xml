    <declare-styleable name="CircleIndicator">
        <attr name="ci_width" format="dimension" />
        <attr name="ci_height" format="dimension" />
        <attr name="ci_margin" format="dimension" />
        <attr name="ci_animator" format="reference" />
        <attr name="ci_animator_reverse" format="reference" />
        <attr name="ci_drawable" format="reference" />
        <attr name="ci_drawable_unselected" format="reference" />
    </declare-styleable>
    <declare-styleable name="MaterialSpinner">
        <attr name="ms_baseColor" format="integer" />
        <attr name="ms_highlightColor" format="integer" />
        <attr name="ms_errorColor" format="integer" />
        <attr name="ms_error" format="string" />
        <attr name="ms_hint" format="string" />
        <attr name="ms_floatingLabelText" format="string" />
        <attr name="ms_floatingLabelColor" format="color" />
        <attr name="ms_multiline" format="boolean" />
        <attr name="ms_nbErrorLines" format="integer" />
        <attr name="ms_typeface" format="string" />
        <attr name="ms_alignLabels" format="boolean" />
        <attr name="ms_thickness" format="dimension" />
        <attr name="ms_thickness_error" format="dimension" />
        <attr name="ms_arrowSize" format="dimension" />
        <attr name="ms_arrowColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CheckBox">
        <attr name="carbon_checkColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="ActionView">
        <attr name="av_color" format="color"/>
        <attr name="av_action" format="enum">
            <enum name="drawer" value="0"/>
            <enum name="back" value="1"/>
            <enum name="close" value="2"/>
            <enum name="plus" value="3"/>
        </attr>
    </declare-styleable>

    <!--Scrim insets vie that allows the drawer the be behind the status bar and visible -->
    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForegrounds" format="reference|color" />
    </declare-styleable>

    <!-- ColorState to be used in the TinOnImageView view -->
    <declare-styleable name="TintOnStateImageView">
        <attr name="colorStateList" format="reference|color" />
    </declare-styleable>
</resources>